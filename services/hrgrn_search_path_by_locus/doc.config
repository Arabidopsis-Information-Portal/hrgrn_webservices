[epydoc] # Epydoc section marker (required by ConfigParser)

# Information about the project.
name: HRGRN Web Services
url: http://plantgrn.noble.org

# The list of modules to document.  Modules can be named using
# dotted names, module filenames, or package directory names.
# This option may be repeated.
modules: main request_handler service request_builder exception gene_info_service

# Write html output to the directory "../../doc/api/search_path_by_locus"
output: html
target: ../../doc/api/search_path_by_locus

# The CSS stylesheet for HTML output.  Can be the name of a builtin
# stylesheet, or the name of a file.
css: grayscale